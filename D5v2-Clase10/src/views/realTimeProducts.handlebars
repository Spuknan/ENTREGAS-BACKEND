<div>
  <h1>Listado de Productos en Tiempo Real</h1>
  <div id="realTimeProductsContainer">
    {{#each products}}
      <div class="card">
        <div class="title">
          {{this.title}}
        </div>
        <div class="description">
          {{this.description}}
        </div>
        <div class="price">
          <p>Precio: ${{this.price}}</p>
        </div>
        <div class="stock">
          <p>Stock: {{this.stock}}</p>
        </div>
      </div>
    {{/each}}
  </div>
</div>


<script src="/socket.io/socket.io.js"></script>
<link rel="stylesheet" href="/css/realTimeProducts.css">